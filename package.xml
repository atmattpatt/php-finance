<?xml version="1.0"?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package>

  <name>finance</name>
  <summary>Financial calculations</summary>
  <description>
    This extension is a proof-of-concept extension which adds a number of financial
	calculation functions to PHP.  It is really just for educational purposes.
  </description>

  <release>
    <version>0.0.1dev</version>
    <state>devel</state>
    <notes>none</notes>
    <date>2014-01-09</date>
  </release>
  <changelog>

  </changelog>
  <deps>
    <dep type="php" rel="ge" version="4.0.0"/>
  </deps>

  <filelist>
    <dir name="/">
      <file role='doc' name='EXPERIMENTAL'/>
      <file role='src' name='config.m4'/>
      <file role='src' name='finance.dsp'/>
      <file role='src' name='config.w32'/>
      <file role='src' name='finance.c'/>
      <file role='src' name='php_finance.h'/>
    </dir>
  </filelist>
</package>
