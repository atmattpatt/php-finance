<?xml version="1.0"?>
<package version="2.0" xmlns="http://pear.php.net/dtd/package-2.0"
    xmlns:tasks="http://pear.php.net/dtd/tasks-1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0
http://pear.php.net/dtd/tasks-1.0.xsd
http://pear.php.net/dtd/package-2.0
http://pear.php.net/dtd/package-2.0.xsd">

  <name>finance</name>
  <channel>pecl.php.net</channel>

  <summary>Financial calculations</summary>

  <description>
    This extension is a proof-of-concept extension which adds a number of financial
	calculation functions to PHP.  It is really just for educational purposes.
  </description>


  <date>2014-01-09</date>
  <version>
    <release>0.0.1dev</release>
    <api>0.0.1dev</api>
  </version>
  <stability>
    <release>devel</release>
    <api>devel</api>
  </stability>

  <license>MIT</license>

  <notes>
none
  </notes>

  <contents>
    <dir name="/">
      <file role='doc' name='EXPERIMENTAL'/>
      <file role='src' name='config.m4'/>
      <file role='src' name='finance.dsp'/>
      <file role='src' name='config.w32'/>
      <file role='src' name='finance.c'/>
      <file role='src' name='php_finance.h'/>
    </dir>
  </contents>

  <dependencies>
    <required>
      <php>
        <min>4.0.0</min>
      </php>
      <pearinstaller>
        <min>1.4.0a1</min>
      </pearinstaller>
    </required>
  </dependencies>

  <providesextension>finance</providesextension>

  <extsrcrelease/>

</package>
